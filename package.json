{
  "name": "server-render",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "DEBUG=server-render/* nodemon ./bin/www",
    "build": "rm -rf dist/ && babel --watch src/ -d dist/",
    "webpack:server": "webpack --debug --hot --output-path-info --devtool=eval --display-modules --watch -d"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "morgan": "~1.6.1",
    "pug": "~2.0.0-beta2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.4.1",
    "request": "^2.72.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint-config-xo": "^0.14.1",
    "eslint-config-xo-react": "^0.7.0",
    "eslint-config-xo-space": "^0.13.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^5.1.1",
    "nodemon": "^1.11.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "xo": "^0.15.1"
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "globals": [
      "window",
      "document",
      "fetch"
    ],
    "extends": [
      "xo",
      "xo-react/space"
    ]
  }
}
