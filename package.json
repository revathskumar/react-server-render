{
  "name": "server-render",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "DEBUG=server-render/* nodemon ./bin/www",
    "build:watch": "rm -rf dist/ && babel --watch src/ -d dist/",
    "build": "rm -rf dist/ && babel src/ -d dist/ && webpack -p",
    "webpack:server": "webpack --debug --devtool=eval --watch"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "^1.8.2",
    "prop-types": "^15.5.10",
    "pug": "^2.0.0-rc.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-config": "^1.0.0-beta.3",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "serve-favicon": "^2.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-config-xo-react": "^0.12.0",
    "eslint-config-xo-space": "^0.16.0",
    "eslint-plugin-babel": "^4.1.0",
    "eslint-plugin-react": "^7.0.1",
    "nodemon": "^1.11.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "xo": "^0.18.2"
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "globals": [
      "window",
      "document",
      "fetch"
    ],
    "extends": [
      "xo",
      "xo-react/space"
    ]
  }
}
